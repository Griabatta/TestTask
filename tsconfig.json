{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "types": ["express"],
    "typeRoots": ["./node_modules/@types", "./types"],
  }
}